{
  "name": "@vallrand/tess",
  "version": "0.0.1",
  "description": "TBS Demo",
  "type": "module",
  "scripts": {
    "copy_files": "node ./tasks/copy-files.js ./output -clear ./demo/index.html ./demo/favicon32.png",
    "pack_textures": "node ./tasks/pack-texture.js ./source/textures ./output/assets",
    "pack_meshes": "node ./tasks/pack-mesh.js ./source ./output/assets/models.bin ./demo/manifest.json",
    "prebuild": "npm run copy_files && npm run pack_textures && npm run pack_meshes",
    "build": "webpack --mode production",
    "start": "webpack serve --mode development"
  },
  "author": "WV",
  "license": "MIT",
  "devDependencies": {
    "pngjs": "^6.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
